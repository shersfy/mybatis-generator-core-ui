<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="DB2Tables" targetRuntime="MyBatis3">
        <!--是否去除自动生成的注释。 true：去除 ; false:保留-->
        <commentGenerator>
            <property name="suppressDate" value="false"/>
            <property name="suppressAllComments" value="false"/>
        </commentGenerator>
        <!--数据库链接URL，用户名、密码-->
        <jdbcConnection password="oawLaWSL@1" driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://10.32.15.91:3306/percent10_api?useUnicode=true&amp;characterEncoding=UTF-8&amp;useSSL=false" userId="edpuser"/>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <!--生成model的包名和位置, rootClass指定继承的基类-->
        <javaModelGenerator targetPackage="top.percent10.api.model" targetProject="C:/mybatis/output">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="false"/>
            <property name="rootClass" value="org.shersfy.model.BaseEntity"/>
        </javaModelGenerator>
        <!--生成映射文件的包名和位置-->
        <sqlMapGenerator targetPackage="mapping" targetProject="C:/mybatis/output">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <!--生成DAO的包名和位置-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="top.percent10.api.mapper" targetProject="C:/mybatis/output">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>
        <!--要生成哪些表-->
        <table tableName="report_conf" domainObjectName="ReportConf" selectByExampleQueryId="false" enableDeleteByExample="false" enableCountByExample="false" enableUpdateByExample="false" enableSelectByExample="false"/>
    </context>
</generatorConfiguration>
